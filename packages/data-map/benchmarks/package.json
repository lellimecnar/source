{
	"name": "@data-map/benchmarks",
	"version": "0.1.0",
	"private": true,
	"description": "DataMap performance benchmarks",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"bench": "vitest bench",
		"bench:arrays": "vitest bench src/arrays-comparative.bench.ts",
		"bench:browser": "vitest bench --config vitest.config.browser.ts",
		"bench:browser:chromium": "vitest bench --config vitest.config.browser.ts --browser.name=chromium",
		"bench:browser:firefox": "vitest bench --config vitest.config.browser.ts --browser.name=firefox",
		"bench:browser:webkit": "vitest bench --config vitest.config.browser.ts --browser.name=webkit",
		"bench:compare": "node scripts/compare-results.js",
		"bench:full": "vitest bench --reporter=json --outputFile=results.json",
		"bench:immutable": "vitest bench src/immutable-updates.bench.ts",
		"bench:memory": "vitest bench src/memory-profiling.bench.ts",
		"bench:path": "vitest bench src/path-access.bench.ts",
		"bench:scale": "vitest bench src/scale-comprehensive.bench.ts",
		"bench:signals": "vitest bench src/signals-comparative.bench.ts",
		"bench:state": "vitest bench src/state-management.bench.ts",
		"bench:subscriptions": "vitest bench src/subscriptions-comparative.bench.ts",
		"build": "vite build",
		"generate-report": "node scripts/generate-report.ts",
		"type-check": "tsgo --noEmit"
	},
	"dependencies": {
		"@data-map/arrays": "workspace:*",
		"@data-map/core": "workspace:*",
		"@data-map/path": "workspace:*",
		"@data-map/signals": "workspace:*",
		"@data-map/storage": "workspace:*",
		"@data-map/subscriptions": "workspace:*"
	},
	"devDependencies": {
		"@lellimecnar/typescript-config": "workspace:*",
		"@lellimecnar/vite-config": "workspace:*",
		"@lellimecnar/vitest-config": "workspace:*",
		"@maverick-js/signals": "^6.0.0",
		"@preact/signals-core": "^1.8.0",
		"@types/dlv": "^1.1.4",
		"@types/lodash": "^4.14.202",
		"@types/node": "^24",
		"@vitest/browser": "^4.0.16",
		"@vue/reactivity": "^3.4.21",
		"deep-object-diff": "^1.1.9",
		"dlv": "^1.1.3",
		"dot-prop": "^8.0.2",
		"dset": "^3.1.3",
		"eventemitter3": "^5.0.1",
		"fast-json-patch": "^3.1.1",
		"immer": "^10.0.4",
		"immutable-json-patch": "^6.0.1",
		"jotai": "^2.8.1",
		"json-pointer": "^0.6.2",
		"klona": "^2.0.6",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"mutative": "^1.3.0",
		"nanoevents": "^9.0.0",
		"nanostores": "^0.10.3",
		"object-path": "^0.11.8",
		"object-path-immutable": "^4.1.2",
		"playwright": "^1.48.2",
		"rfc6902": "^5.1.1",
		"rfdc": "^1.4.1",
		"solid-js": "^1.8.15",
		"typescript": "~5.5",
		"valtio": "^1.11.3",
		"vite": "^7.3.0",
		"vite-plugin-dts": "^4.5.4",
		"vite-tsconfig-paths": "^6.0.3",
		"vitest": "^4.0.16",
		"zustand": "^4.4.7"
	}
}
