{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"plugins": [
			{
				"name": "next"
			}
		],
		"declaration": false,
		"declarationMap": false,
		"incremental": true,
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"]
	},
	"include": ["src", "next-env.d.ts"],
	"exclude": ["node_modules"]
}
